#include <stdio.h>
void main()
{
    int a=10;
    for(int i=1;i<=a;i++)
    {
        printf("*");
    }
    for(int i=1;i<=a;i++)
    {
        printf("\n*        *");
    }
    printf("\n");
     for(int i=1;i<=a;i++)
    {
        printf("*");
    }
}
#output
**********
*        *
*        *
*        *
*        *
*        *
*        *
*        *
*        *
*        *
*        *
**********
