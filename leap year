#include <stdio.h>
void main()
{
    int a;
    printf("Enter the year:");
    scanf("%d",&a);
    if(a%100==0)
    {
        if(a%400==0)
        printf("it is leap year");
        else
        printf("it is not leap year");
    }
    else
    {
        if(a%4==0)
        printf("it is leap year");
        else
        printf("it is not leap year");
    }
}
