#include <stdio.h>

void main()
{
  int a,b,c;
  printf("enter any 3 no.:");
  scanf("%d%d%d",&a,&b,&c);
  {
      if(a>b && a>c)
  {
      if(b>c)
      printf("the second largest no.:%d", b);
      else
      printf("the second largest no.:%d", c);
  }
  if(b>a && b>c)
  {
      if(a>c)
      printf("the second largest no.:%d", a);
      else
      printf("the second largest no.:%d", b);
  }
  if(c>b && c>a)
  {
      if(b>a)
      printf("the second largest no.:%d", b);
      else
      printf("the second largest no.:%d", a);
  }
      
  }
